{
  "name": "DashTab",
  "description": "Override the new tab page",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'; img-src chrome://favicon; "
}
